<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	

	<!-- constructor-arg 생성자 -->
	<bean id="birth" class="com.test02.BirthDto">
		<constructor-arg name="name" value="조영찬"></constructor-arg>
		<constructor-arg name="birth" value="27"></constructor-arg>
	</bean>
	
	<!-- property = setter -->
	<bean id="birth2" class="com.test02.BirthDto">
		<property name="name" value="조혜리"></property>
		<property name="birth" value="24"></property>
	</bean>
	<!-- property = setter -->
	<bean id="birth3" class="com.test02.BirthDto">
		<property name="name" value="홍준화"></property>
		<property name="birth" value="27"></property>
	</bean>
</beans>
