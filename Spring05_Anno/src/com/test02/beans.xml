<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config/>
	
	<bean id="bark" class="com.test02.Student">
		<property name="name" value="박혁거세"></property>
		<property name="addr" value="알"></property>
		<property name="age" value="1200"></property>
	</bean>
	
	<bean id="se" class="com.test02.Student">
		<property name="name" value="세종대왕"></property>
		<property name="addr" value="조선"></property>
		<property name="age" value="800"></property>
	</bean>
	
	<bean id="school" class="com.test02.School">
		<property name="grade" value="1"></property>
	</bean>
	
</beans>
